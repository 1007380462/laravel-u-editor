{
  "name": "stevenyangecho/laravel-u-editor",
  "description": "UEditor for laravel5. Support i18n. UEditor is a Rich Text Web Editor From Baidu.",
  "keywords": [
    "laravel",
    "ueditor",
    "laravel-u-editor",
    "laravel5 web text editor",
    "stevenyangecho",
    "steven yang"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Steven Yang",
      "email": "yqmking@163.com"
    }
  ],

  "require": {
    "php": ">=5.4.0",
    "illuminate/support": "5.0.*",
    "qiniu/php-sdk": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Stevenyangecho\\UEditor\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
